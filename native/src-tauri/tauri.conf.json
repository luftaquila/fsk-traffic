{
  "build": {
    "devPath": ["../src", "../node_modules"],
    "distDir": ["../src", "../node_modules"],
    "withGlobalTauri": true
  },
  "package": {
    "productName": "fsk-traffic-control",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "fsk-traffic-control",
        "width": 1800,
        "height": 1100,
        "x": 0,
        "y": 0
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "luftaquila.fsk-traffic-control",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
